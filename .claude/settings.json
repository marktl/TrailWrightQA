{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"],
      "env": {}
    }
  },
  "permissions": {
    "allow": [
      "mcp__playwright__*"
    ],
    "deny": []
  },
  "timeouts": { "mcpRequestMs": 240000 }
}
